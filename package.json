{
  "name": "n8n-nodes-reflag",
  "version": "1.0.0",
  "description": "n8n nodes for the Reflag service used in automation",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://reflag.com",
  "author": {
    "name": "Reflag",
    "email": "support@reflag.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reflagcom/n8n-nodes-reflag.git"
  },
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "sync": "node sync.mjs",
    "build": "npx rimraf dist && npm run sync && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ReflagAPI/ReflagAPI.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ReflagAPI/ReflagAPI.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "chalk": "^5.6.2",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "n8n-workflow": "^1.111.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@devlikeapro/n8n-openapi-node": "^0.1.4"
  }
}